<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="eo_job_queue.entity.many_to_any_listener.class">Eo\JobQueueBundle\Entity\Listener\ManyToAnyListener</parameter>
    </parameters>

    <services>
        <service id="eo_job_queue.entity.many_to_any_listener" class="%eo_job_queue.entity.many_to_any_listener.class%">
            <argument type="service" id="doctrine" />
            <tag name="doctrine.event_listener" lazy="true" event="postGenerateSchema" />
            <tag name="doctrine.event_listener" lazy="true" event="postLoad" />
            <tag name="doctrine.event_listener" lazy="true" event="postPersist" />
        </service>
    </services>
</container>
