<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}EoJobQueueBundle{% endblock %}</title>
        <link href="{{ asset("bundles/eojobqueue/css/bootstrap.css") }}" rel="stylesheet" />
    </head>
    <body>
        <div class="navbar">
            <div class="navbar-inner">
                <a class="brand" href="{{ path("eo_jobs_overview") }}">EoJobQueueBundle</a>
            </div>
        </div>
        <div class="container">
            {% block content %}{% endblock %}
        </div>

        {% block javascripts %}
        <script src="{{ asset("bundles/eojobqueue/js/jquery.js") }}" type="text/javascript"></script>
        <script src="{{ asset("bundles/eojobqueue/js/bootstrap.js") }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>